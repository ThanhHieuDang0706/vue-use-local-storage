{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "ESNext",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*", "src/types/*"]
        },
        "skipLibCheck": true,
        "noEmit": true,
        "jsx": "preserve",
        "jsxImportSource": "vue"
    },
    "include": ["src/**/*"],

}
